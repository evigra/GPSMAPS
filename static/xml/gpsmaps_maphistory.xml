<templates id="maphistory" name="maphistory">
    <t t-name="gpsmaps_maphistory">        
        <div style="position:relative; width:100%; height:100%;">                        
            <t t-call="menu_vehicles"/>                    
            <div id="maponline" style="top:0px; left:250px; width: calc(100% - 250px); height:100%;"/>  
            <div id="filtro">
                <t t-call="gpsmaps_filters"/>
            </div>
            <div id="tableros" class="o_main_navbar" style="z-index:100; text-align:left; position:absolute; bottom:0px; left:0px; width:100%; height:70px; color:#aaa; padding:10px;">                
                <div id="tablero2"  style="float:left;"></div>                       
                <div id="tablero"  style="float:left;"></div>
            </div>
            <t t-call="gpsmaps_buttons"/>                              
            <t t-call="gpsmaps_odometers"/>
        </div>
    </t>
</templates>

