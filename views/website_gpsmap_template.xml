<odoo>
    <template id="maponline" name="maponline">

        <t t-name="gpsmap.gpsmaps_maponline">        
            <div style="position:relative; width:100%; height:100%;">                        
                <t t-call="gpsmap.menu_vehicles"/>                                    
                <div id="maponline" style="top:0px; left:250px; width: calc(100% - 250px); height:100%;">
                    MAPA
                </div>  
                <div id="tableros" class="o_main_navbar" style="z-index:100; font-size:25px; text-align:left; position:absolute; bottom:0px; left:0px; width:100%; height:70px; color:#ccc; padding:10px;">
                    TABLEROS
                </div>                              
                
                <t t-call="gpsmap.gpsmaps_odometers"/>
                
            </div>
        </t>
    </template>
    <template id="menu_vehicles" name="menu_vehicles">

        <t t-name="gpsmap.menu_vehicles">        
            
            <div id="menu_vehicles" style="width:250px; height: calc(100% - 70px); position:absolute; top:0px; left:0px; display: block;"> 
                <div class="vehicle vehicle_active" vehicle="0" style="float:left; padding-top:8px; padding-left:8px; height:35px; width:250px: display: block;">
                    <div class="filter_vehicle" style="float:left; height:35px; width:35px: display: block;">
                        Todos los vehiculos
                    </div>
                </div>             
                <div id="vehicles" style="width:100%; height: calc(100% - 35px); position:absolute; top:35px; left:0px; overflow-y: scroll; display: block;"> 
                <t t-if="vehicles">            
                    
                    <t t-foreach="vehicles" t-as="vehicle">                        
                        <t t-set="gps" t-value="vehicle.gps1_id"/>
                        <t t-set="position" t-value="vehicle.positionid"/>

                        <div 
                            class="vehicle" 
                            style="float:left; display: block; width:100%; height:35px;" 
                            position="" latitude="" longitude="" speed="" time="" date="" distance=""
                            t-att-device_id="gps[0]" 
                            t-att-vehicle="vehicle.id"
                        >
                            <div style="padding-top:5px; padding-left:5px; float:left; width:40px; height:100%; display: block;">                            
                                <t t-set='image_path' t-value="'/gpsmap/static/img/vehicles/vehicle_' + vehicle.image_vehicle +  '/i135.png'"/>
                                <img height="18" t-att-src="image_path" />
                            </div>
                            <div style="float:left; position:relative; width:180px; height:100%; display: block;">                            
                                <div style="position:absolute; top:1px; left:0px; font-size:15px;">
                                    <t t-esc="vehicle.economic_number"/>                                
                                </div>
                                <div style="position:absolute; top:16px; left:0px; font-size:9px;">
                                    <b><t t-esc="vehicle.license_plate"/></b>
                                </div>
                            </div>
                            <div class="event_device" style="padding-top:6px; float:left; width:30px; height:100%; display: block;"/>
                        </div>                
                    
                    </t>
                </t>
                </div>    
            </div> 

        </t>        

    </template>


    <template id="gpsmaps_odometers" name="gpsmaps_odometers">
        <t t-name="gpsmaps_odometers">        
            
            <div id="odometer" style="z-index:101; position:absolute; bottom:0px; height:0px; width:300px; right:0px;  ">
                <div id="speed" style="z-index:110; overflow:hidden;position:absolute; bottom:-10px; right:150px; width:250px; height:225px">
                    <t t-call="gpsmap.gpsmaps_odometers_speed"/>                    
                </div>
                <!--
                <div id="speed" style="z-index:100; overflow:hidden;position:absolute; bottom:-50px; right:300px; width:250px; height:225px">
                    <t t-call="gpsmaps_template_batery"/>                    
                </div>

                <div id="speed" style="z-index:100; overflow:hidden;position:absolute; bottom:-50px; right:0px; width:250px; height:225px">
                    <t t-call="gpsmaps_odometers_gas"/>                    
                </div>
                -->
            </div>                
        </t>

    </template>


